  "scripts": {
    "start": "ts-node src/main.ts",
    "dev": "(cd dev/db && docker-compose up -d) && MB_ENV_FILE=dev/dev.env nodemon -e graphql,js,ts --ignore './src/frontend/' src/main.ts",
    "test": "MB_ENV_FILE=dev/test.env yarn jest --runInBand --no-cache --forceExit --detectOpenHandles",
    "knex": "MB_ENV_FILE=dev/dev.env knex --knexfile=./dev/db/dev.knexfile.ts",
    "knex:prod": "MB_ENV_FILE=/etc/mintbean-v4/config/prod.env knex --knexfile /etc/mintbean-v4/config/prod.env",
    "knex:test": "MB_ENV_FILE=dev/test.env knex --knexfile ./dev/test/test.knexfile.ts",
    "pristine": "dev/db/init-db.sh && yarn knex migrate:latest && yarn knex seed:run && yarn knex:test migrate:latest && yarn knex:test seed:run",
    "lint": "tsc --noEmit && prettier --check .",
    "dev:generate-dataset": "ts-node dev/test/datasetGenerator.ts"
  },
